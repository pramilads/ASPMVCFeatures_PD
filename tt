export class EmployeeSearchModel {
  WmcId: string;
  HeadquarterId: string;
  EnergyTypeId: bigint;
  DateFrom: string;
  DateTo: string;
  WorkPeriod: string;
}

export class OrderSearchModel {
  WmcId: string;
  HeadquarterId: string;
  EnergyTypeId: bigint;
  DateFrom: string;
  DateTo: string;
  WorkPeriod: string;
}

export class OrderCrewSearchModel {
  WorkType: string;
}


export class CrewUpdateModel {
  Id: string;
  Active: number;
  EmployeeClassficationId: string;
}

export class OrderDetailsUpdateModel {
  Hours: string;
  StartDate: string;
  EndDate: string;
  UnloadingPoint: string;
  Comments: string;
  WorkOrderId: string;
}

export class OrderListUpdateModel {
  OrderListToUpdate: OrderInfoUpdateModel[];
}

//checkboxes in order list table
export class OrderInfoUpdateModel {
