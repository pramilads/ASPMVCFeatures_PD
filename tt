import { Pipe, PipeTransform } from '@angular/core';
@Pipe({
    name: 'ddlFilter'
})
export class FilterByDropDownNamePipe implements PipeTransform {

    transform(dropDownData : any, dropDownName: string): any[] {
        if (dropDownData) {
            return dropDownData.filter((ddl) => ddl.dropdownName === dropDownName);
        }
    }
}
